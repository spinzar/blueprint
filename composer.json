{
    "name": "spinzar/blueprint",
    "description": "API Blueprint documentation generator.",
    "keywords": ["blueprint", "api", "laravel", "dingo", "docs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nasibullah Nassim",
            "email": "spinzar.inc@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0",
        "doctrine/annotations": "^1.12",
        "phpdocumentor/reflection-docblock": "^3.1 || ^4.1 || ^5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^6.5|^8.3|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dingo\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Blueprint\\Tests\\": "tests"
        }
    },
     "extra": {
        "branch-alias": {
            "dev-main": "-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
